    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintjs": "eslint --fix --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lintcss": "stylelint --fix ./**/*.vue ./**/*.scss",
    "precommit": "npm run lint",
    "test": "jest"
